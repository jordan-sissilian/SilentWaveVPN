{
    "version": 3,
    "name": "API Gateway",
    "endpoints": [
      {
        "endpoint": "/info",
        "method": "GET",
        "backend": [
          {
            "host": ["http://localhost:8080"],
            "url_pattern": "/info/check"
          }
        ],
        "extra_config": {
          "proxy": {
            "static": {
              "data": {
                "vpn_name": {
                  "1": "first",
                  "2": "two"
                }
              }
            }
          }
        }
      },
      {
        "endpoint": "/vpn/{id}",
        "method": "GET",
        "backend": [
          {
            "host": ["http://localhost:8080"],
            "url_pattern": "/info1",
            "extra_config": {
              "github.com/devopsfaith/krakend/proxy": {
                "static": {
                  "id": "1"
                }
              }
            }
          },
          {
            "host": ["http://localhost:8080"],
            "url_pattern": "/info2",
            "extra_config": {
              "github.com/devopsfaith/krakend/proxy": {
                "static": {
                  "id": "2"
                }
              }
            }
          }
        ]
      },
      {
        "endpoint": "/info1",
        "method": "GET",
        "backend": [
          {
            "host": ["http://localhost:8080"],
            "url_pattern": "/info/check"
          }
        ],
        "extra_config": {
          "proxy": {
            "static": {
              "data": {
                "vpn_name": {
                  "1": "first"
                }
              }
            }
          }
        }
      },
      {
        "endpoint": "/info2",
        "method": "GET",
        "backend": [
          {
            "host": ["http://localhost:8080"],
            "url_pattern": "/info/check"
          }
        ],
        "extra_config": {
          "proxy": {
            "static": {
              "data": {
                "vpn_name": {
                  "2": "two"
                }
              }
            }
          }
        }
      }
    ]
  }
  